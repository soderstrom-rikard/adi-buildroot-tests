set line /dev/ttyUSB0
define sz !sz \%0 > /dev/ttyUSB0 < /dev/ttyUSB0
define rz !rz < /dev/ttyUSB0 > /dev/ttyUSB0
set speed 9600
set protocol zmodem
set carrier-watch off
set prefixing all
set parity none
set stop-bits 1
set modem none
set file type bin
set file name lit
set flow-control none
set escape 0
set prompt "ttyUSB0 > "
