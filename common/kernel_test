    copy_image_config "serial"
    mv $TESTSUITES_DIR/serial/*_log  $THIS_LOG

    echo  "End testing serial..."
    sleep 10

    if [ $RESET_BOARD -eq 1 ] ; then
        cd $TESTSUITES_DIR/common
        ./reset_board.exp $BOARD_TYPE > /dev/null 2>&1
        sleep 10
    fi

fi

